apiVersion: v1
baseDomain: {{ cluster_domain }}
compute: 
- name: worker
  replicas: 0 
controlPlane: 
  name: master
  replicas: 1 
metadata:
  name: {{ cluster_name }} 
networking:
  clusterNetwork:
  - cidr: 10.128.0.0/14 
    hostPrefix: 23 
  networkType: OVNKubernetes 
  serviceNetwork: 
  - 172.30.0.0/16
platform:
  none: {} 
fips: {{ cluster_fips_flag }} 
pullSecret: {{ cluster_pull_secret | quote }}
sshKey: {{ cluster_ssh_key | quote }}
