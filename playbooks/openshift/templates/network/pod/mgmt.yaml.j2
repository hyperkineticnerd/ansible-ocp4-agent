    {
      "cniVersion": "0.3.1",
      "name": "management",
      "type": "host-device",
      "host-device": "ens2"
      "ipam": {
        "type": "static",
        "addresses": [
          {
            "address": "{{ cluster_mgmt_ip }}/24"
          }
        ]
      }
    }'
