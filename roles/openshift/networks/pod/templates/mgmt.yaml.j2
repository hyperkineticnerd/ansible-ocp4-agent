apiVersion: k8s.cni.cncf.io/v1
kind: NetworkAttachmentDefinition
metadata:
  name: mgmt-vlan
  namespace: trident-system
spec:
  config: |-
    {
      "cniVersion": "0.3.1",
      "name": "mgmt-vlan",
      "type": "ipvlan",
      "master": "bond0.670",
      "linkInContainer": false,
      "mode": "l2",
      "ipam": {
        "type": "static",
        "addresses": [
          {
            "address": "{{ cluster_mgmt_ip }}/24"
          }
        ]
      }
    }
